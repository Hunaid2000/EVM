            <Button id="Party Results" fx:id="PartyResults" alignment="CENTER" contentDisplay="CENTER" layoutX="236.0" layoutY="306.0" mnemonicParsing="false" onAction="#PartyButtonPressed" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: #0d6f06; -fx-background-radius: 15;" text="Party Results" textFill="WHITE" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
