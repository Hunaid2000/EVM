         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#0d6f06" height="40.0" stroke="BLACK" strokeType="INSIDE" width="350.0" />
            <Text fill="WHITE" layoutX="151.0" layoutY="24.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Results">
               <font>
                  <Font name="Dubai Regular" size="16.0" />
               </font>
            </Text>
            <Button id="AdminView" fx:id="AdminView" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="306.0" mnemonicParsing="false" onAction="#AdminViewButtonPressed" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: #0d6f06; -fx-background-radius: 15;" text="Admin View" textFill="WHITE" />
            <TableView id="Result" fx:id="Result" layoutX="25.0" layoutY="51.0" prefHeight="248.0" prefWidth="301.0">
